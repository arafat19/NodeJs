<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css'/>
</head>
<body>
<!-- = sign is only title not for Pure JS code-->
<h1><%= title %></h1>

<% include template/header.ejs %>

<h1> <%= videodata.categoryName %> </h1>

<ul>
    <% videodata.categories.forEach(function(item){ %>

        <li><a href="<%= item.categoryID%>"><%= item.categoryName%></a></li>

    <% }); %>
</ul>
</body>
</html>
